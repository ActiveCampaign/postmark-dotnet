<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <clear/>
    <!-- Set this to your email server's API token (Guid) -->
    <add key="WRITE_TEST_SERVER_TOKEN" value=""/>

    <!-- Set this to a valid sender signature (email address) -->
    <add key="WRITE_TEST_SENDER_EMAIL_ADDRESS" value="" />

    <!-- Set this to a known safe message recipient (email address) -->
    <add key="WRITE_TEST_EMAIL_RECIPIENT_ADDRESS" value="" />
  </appSettings>
</configuration>